{
  "name": "react-usage-bar",
  "version": "1.1.0",
  "description": "Usage bar, graphic component for React",
  "main": "build/index.js",
  "module": "build/index.mjs",
  "typings": "build/index.d.ts",
  "files": [
    "build/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisUser/react-usage-bar.git"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "standard",
    "format": "prettier-standard --format",
    "test": "jest --coverage",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook -c .storybook -o docs-build .out"
  },
  "keywords": [
    "react",
    "usage bar",
    "design",
    "component"
  ],
  "author": {
    "name": "ChrisUser",
    "url": "https://github.com/ChrisUser"
  },
  "homepage": "http://ChrisUser.github.io/react-usage-bar",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-viewport": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^12.1.3",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "jest": "^27.5.1",
    "prettier-standard": "^16.3.0",
    "react": "^17.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.10.9",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "semantic-release": "^19.0.2",
    "standard": "^16.0.4",
    "standard-prettier": "^1.0.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^8.2.0",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "react": "^17",
    "react-dom": "^17"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "build/"
    ]
  },
  "standard": {
    "ignore": [
      "node_modules/",
      "build/"
    ],
    "globals": [
      "describe",
      "it",
      "test",
      "expect",
      "afterAll",
      "jest"
    ]
  },
  "dependencies": {
    "esbuild": "^0.14.23",
    "postcss": "^8.4.7",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-esbuild": "^4.8.2"
  }
}
